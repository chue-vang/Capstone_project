<style type="text/css">
    #map {
        height: 400px;
        width: 70%;
    }
</style>
<script>
    function initMap() {
        const myLatLng = { lat: 44.24751, lng: -88.37369 };
        const map = new google.maps.Map(document.getElementById("map"), {
            zoom: 14,
            center: myLatLng,
        });
        new google.maps.Marker({
            position: myLatLng,
            map,
        });
    }
</script>

